pipeline {
    agent any
    tools{
        maven 'maven 3.6'
    }

    stages {
        stage('code checkout'){
            steps{
                git credentialsId: '7051b5f2-b6a5-4d5e-b67e-bc5775ade943', url: 'https://github.com/pavankalyanimmadi/helloworld.git'
            }
        }
    
        stage('Build') {
            steps {
               sh "mvn clean install"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
